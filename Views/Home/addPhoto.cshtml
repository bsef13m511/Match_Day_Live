@model MatchDayLive.Models.Picture
@{
    ViewBag.Title = "addPhoto";
    Layout = "~/Views/Shared/teamLayout.cshtml"; 
}

<h2>Add Photo to Team Gallery</h2>
<div class="container-fluid">
@using (Html.BeginForm("addPhoto", "Home", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data", role = "form" }))
{
      
       @Html.TextBoxFor(model => model.PhotoTitle, new {type = "file" })
        <p class="error">
       @Html.ValidationMessageFor(model => model.PhotoTitle)
            </p>
     <div class="btn btn-submit">
        <button type="submit" class="btn btn-default" id="b1">Submit</button>
      </div>
}

</div>